{
    "name": "raveren/laravel-debugbar-phpstorm",
    "description": "laravel-debugbar plugin that provides buttons to open files in phpstorm (remote call plugin required)",
    "keywords": ["laravel", "debugbar", "phpstorm"],
    "license": "MIT",
    "authors": [
        {
            "name": "Erlang Parasu",
            "email": "erlangparasu@gmail.com"
        }, 
        {
            "name": "raveren",
            "email": "raveren@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "barryvdh/laravel-debugbar": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ErlangParasu\\DebugbarVscode\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ErlangParasu\\DebugbarVscode\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "5.5.x"
    }
}
